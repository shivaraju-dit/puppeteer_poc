{
  "name": "puppeteer_poc",
  "version": "1.0.0",
  "description": "Automation testing with puppeteer ",
  "main": "index.js",
  "scripts": {
    "test:snapshots": "jest --config=jest.config.js --detectOpenHandles --forceExit",
    "test:snapshots-update": "jest --config.jest.config.js -- updateSnapshot --detectOpenHandles --forceExit",
    "test:percy": "percy exec -- jest -i './tests-snapshots/_tests_/percy.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shivaraju-dit/puppeteer_poc.git"
  },
  "keywords": [
    "Puppeteer",
    "Jest",
    "Visual testing"
  ],
  "author": "Shiva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shivaraju-dit/puppeteer_poc/issues"
  },
  "homepage": "https://github.com/shivaraju-dit/puppeteer_poc#readme",
  "dependencies": {
    "@percy/puppeteer": "^1.1.0",
    "jest": "^26.6.3",
    "jest-image-snapshot": "^4.3.0",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0"
  }
}
